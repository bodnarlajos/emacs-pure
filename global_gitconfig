        fsmonitor = false
        preloadindex = true
        fscache = true
	editor = "emacsclientw.exe -n --alternate-editor=\"runemacs.exe\""
[credential]
        helper = !\"c:/Program Files/Git/mingw64/libexec/git-core/git-credential-manager-core.exe\"
[credential "helperselector"]
	selected = manager-core
[filter "lfs"]
        clean = git-lfs clean -- %f
        smudge = git-lfs smudge -- %f
        process = git-lfs filter-process
        required = true
[gc]
        auto = 256
[pull]
	ff = only
[gui]
	recentrepo = C:/Projects/bedwh_ssis
	fontui = -family \"JetBrains Mono\" -size 10 -weight normal -slant roman -underline 0 -overstrike 0
	fontdiff = -family \"JetBrains Mono\" -size 11 -weight normal -slant roman -underline 0 -overstrike 0
[alias]
	l = log --first-parent --color --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	lg = log --graph --color --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	c = commit -m
	p = pull
	f = fetch --all
	g = !gitk &
	gg = !git gui &
	s = status
